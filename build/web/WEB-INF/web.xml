    <jsp-config>
        <jsp-property-group>
            <display-name>jsps codificados</display-name>
            <url-pattern>*.jsp</url-pattern>
            <page-encoding>ISO-8859-1</page-encoding>
        </jsp-property-group>
    </jsp-config>
